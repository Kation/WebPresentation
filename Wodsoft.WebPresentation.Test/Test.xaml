<Page xmlns="http://schemas.wodsoft.com/web/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:html="http://schemas.wodsoft.com/web/presentation/html"
      Title="{Binding Text, ElementName=TitleText}" DataContext="test">
    <Page.Template>
        <ControlTemplate>
            <HtmlWrapper>
                <html:HtmlPage>
                    <html:HtmlSpan Text="{TemplateBinding DataContext}"></html:HtmlSpan>
                </html:HtmlPage>
            </HtmlWrapper>
        </ControlTemplate>
    </Page.Template>
    <TextBlock Name="TitleText" Text="TestPage"/>
</Page>